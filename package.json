    "core-js": "^3.6.5",
    "meyda": "^5.0.3",
    "onsenui": "^2.10.10",
    "pouch-vue": "^0.3.5",
    "pouchdb": "^7.2.1",
    "pouchdb-browser": "^7.2.1",
    "pouchdb-find": "^7.2.1",
    "pouchdb-live-find": "^0.4.0",
    "register-service-worker": "^1.7.1",
    "vue": "^2.6.11",
    "vue-onsenui": "^2.6.2",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-pwa": "~4.4.0",
    "@vue/cli-plugin-router": "~4.4.0",
